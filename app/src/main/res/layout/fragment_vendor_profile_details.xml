<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/white"
    tools:context=".onboarding.auth.vendor.profileDetails.VendorProfileDetailsFragment">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_30sdp"
        android:fontFamily="@font/rasa_medium"
        android:text="@string/get_lyft_d"
        android:textColor="@color/black"
        android:textSize="@dimen/_38ssp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_30sdp"
        android:fontFamily="@font/montserrat_medium"
        android:text="@string/add_store_images"
        android:textColor="@color/black"
        android:textSize="@dimen/_18ssp"
        app:layout_constraintEnd_toEndOf="@+id/textView"
        app:layout_constraintStart_toStartOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="@dimen/_120sdp"
        android:layout_height="@dimen/_120sdp"
        android:layout_marginTop="@dimen/_17sdp"
        android:src="@drawable/ic_add_store_image"
        app:layout_constraintEnd_toEndOf="@+id/textView3"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/sign_in_btn"
        android:layout_width="0dp"
        android:layout_height="@dimen/_50sdp"
        android:layout_marginStart="@dimen/_20sdp"
        android:layout_marginEnd="@dimen/_20sdp"
        android:backgroundTint="@color/main_green"
        android:fontFamily="@font/montserrat_medium"
        android:text="@string/upload_more_photos"
        android:textSize="@dimen/_14ssp"
        android:layout_marginTop="@dimen/_17sdp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView6" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_add"
        app:layout_constraintBottom_toTopOf="@+id/upload_one"
        app:layout_constraintEnd_toEndOf="@+id/upload_one"
        app:layout_constraintStart_toEndOf="@+id/upload_one"
        app:layout_constraintTop_toTopOf="@+id/upload_one" />

    <View
        android:id="@+id/upload_one"
        android:layout_width="@dimen/_80sdp"
        android:layout_height="@dimen/_80sdp"
        android:layout_marginTop="@dimen/_17sdp"
        android:background="@drawable/green_border_grey_bg"
        android:src="@drawable/ic_add_more"
        android:layout_marginEnd="@dimen/_10sdp"
        app:layout_constraintStart_toStartOf="@+id/sign_in_btn"
        app:layout_constraintTop_toBottomOf="@+id/sign_in_btn"
        app:layout_constraintEnd_toStartOf="@+id/imageView8"/>

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:padding="@dimen/_16sdp"
        android:src="@drawable/ic_add_more"
        app:layout_constraintBottom_toBottomOf="@+id/upload_one"
        app:layout_constraintEnd_toEndOf="@+id/upload_one"
        app:layout_constraintStart_toStartOf="@+id/upload_one"
        app:layout_constraintTop_toTopOf="@+id/upload_one" />

    <View
        android:id="@+id/upload_two"
        android:layout_width="@dimen/_80sdp"
        android:layout_height="@dimen/_80sdp"
        android:layout_marginTop="@dimen/_17sdp"
        android:background="@drawable/green_border_grey_bg"
        android:src="@drawable/ic_add_more"
        app:layout_constraintEnd_toStartOf="@+id/upload_three"
        app:layout_constraintStart_toEndOf="@+id/imageView7"
        app:layout_constraintTop_toBottomOf="@+id/sign_in_btn"/>

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:padding="@dimen/_16sdp"
        android:src="@drawable/ic_add_more"
        app:layout_constraintBottom_toBottomOf="@+id/upload_two"
        app:layout_constraintEnd_toEndOf="@+id/upload_two"
        app:layout_constraintStart_toStartOf="@+id/upload_two"
        app:layout_constraintTop_toTopOf="@+id/upload_two" />

    <View
        android:id="@+id/upload_three"
        android:layout_width="@dimen/_80sdp"
        android:layout_height="@dimen/_80sdp"
        android:layout_marginTop="@dimen/_17sdp"
        android:layout_marginStart="@dimen/_10sdp"
        android:background="@drawable/green_border_grey_bg"
        app:layout_constraintEnd_toEndOf="@+id/sign_in_btn"
        app:layout_constraintStart_toEndOf="@+id/imageView8"
        app:layout_constraintTop_toBottomOf="@+id/sign_in_btn"/>

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:padding="@dimen/_16sdp"
        android:src="@drawable/ic_add_more"
        app:layout_constraintBottom_toBottomOf="@+id/upload_three"
        app:layout_constraintEnd_toEndOf="@+id/upload_three"
        app:layout_constraintStart_toStartOf="@+id/upload_three"
        app:layout_constraintTop_toTopOf="@+id/upload_three" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_add"
        app:layout_constraintBottom_toTopOf="@+id/upload_one"
        app:layout_constraintEnd_toEndOf="@+id/upload_one"
        app:layout_constraintStart_toEndOf="@+id/upload_one"
        app:layout_constraintTop_toTopOf="@+id/upload_one" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_add"
        app:layout_constraintBottom_toTopOf="@+id/upload_three"
        app:layout_constraintEnd_toEndOf="@+id/upload_three"
        app:layout_constraintStart_toEndOf="@+id/upload_three"
        app:layout_constraintTop_toTopOf="@+id/upload_three" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_add"
        app:layout_constraintBottom_toTopOf="@+id/upload_two"
        app:layout_constraintEnd_toEndOf="@+id/upload_two"
        app:layout_constraintStart_toEndOf="@+id/upload_two"
        app:layout_constraintTop_toTopOf="@+id/upload_two" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/submit_btn"
        android:layout_width="0dp"
        android:layout_height="@dimen/_50sdp"
        android:layout_marginStart="@dimen/_20sdp"
        android:layout_marginEnd="@dimen/_20sdp"
        android:backgroundTint="@color/main_green"
        android:fontFamily="@font/montserrat_medium"
        android:text="@string/submit"
        android:layout_marginBottom="@dimen/_20sdp"
        android:textSize="@dimen/_14ssp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>