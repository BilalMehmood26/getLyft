        android:layout_marginStart="@dimen/_7sdp"
        app:layout_constraintStart_toEndOf="@+id/cat_one_tv"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/cat_two_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/montserrat_medium"
        android:text="Category 2"
        android:textColor="@color/wood_black"
        android:layout_marginStart="@dimen/_4sdp"
        android:textSize="@dimen/_10ssp"
        app:layout_constraintBottom_toBottomOf="@+id/cat_two"
        app:layout_constraintStart_toEndOf="@+id/cat_two"
        app:layout_constraintTop_toTopOf="@+id/cat_two" />

    <CheckBox
        android:id="@+id/cat_three"
        android:layout_width="@dimen/_20sdp"
        android:layout_height="@dimen/_20sdp"
        android:layout_marginTop="@dimen/_17sdp"
        android:layout_marginStart="@dimen/_7sdp"
        app:layout_constraintStart_toEndOf="@+id/cat_two_tv"
