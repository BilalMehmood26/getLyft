    <CheckBox
        android:id="@+id/rat_three"
        android:layout_width="@dimen/_20sdp"
        android:layout_height="@dimen/_20sdp"
        android:layout_marginTop="@dimen/_17sdp"
        android:layout_marginStart="@dimen/_7sdp"
        app:layout_constraintStart_toEndOf="@+id/rat_two_tv"
        app:layout_constraintTop_toBottomOf="@+id/rating_tv" />

    <TextView
        android:id="@+id/rat_three_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/montserrat_medium"
        android:text="3 Star"
        android:textColor="@color/wood_black"
        android:layout_marginStart="@dimen/_4sdp"
        android:textSize="@dimen/_12ssp"
        app:layout_constraintBottom_toBottomOf="@+id/rat_three"
        app:layout_constraintStart_toEndOf="@+id/rat_three"
        app:layout_constraintTop_toTopOf="@+id/rat_three" />


    <CheckBox
        android:id="@+id/rat_four"
        android:layout_width="@dimen/_20sdp"
        android:layout_height="@dimen/_20sdp"
        android:layout_marginTop="@dimen/_17sdp"
        app:layout_constraintStart_toStartOf="@+id/rating_tv"
        app:layout_constraintTop_toBottomOf="@+id/rat_one" />

    <TextView
        android:id="@+id/rat_four_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/montserrat_medium"
        android:text="2 star"
        android:textColor="@color/wood_black"
        android:layout_marginStart="@dimen/_4sdp"
        android:textSize="@dimen/_12ssp"
        app:layout_constraintBottom_toBottomOf="@+id/rat_four"
        app:layout_constraintStart_toEndOf="@+id/rat_four"
        app:layout_constraintTop_toTopOf="@+id/rat_four" />

    <CheckBox
        android:id="@+id/rat_five"
        android:layout_width="@dimen/_20sdp"
        android:layout_marginStart="@dimen/_7sdp"
        android:layout_height="@dimen/_20sdp"
        app:layout_constraintBottom_toBottomOf="@+id/rat_four_tv"
        app:layout_constraintStart_toEndOf="@+id/rat_four_tv"
        app:layout_constraintTop_toTopOf="@+id/rat_four_tv" />

    <TextView
        android:id="@+id/rat_five_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/montserrat_medium"
        android:text="1 Star"
        android:textColor="@color/wood_black"
        android:layout_marginStart="@dimen/_4sdp"
        android:textSize="@dimen/_12ssp"
        app:layout_constraintBottom_toBottomOf="@+id/rat_five"
        app:layout_constraintStart_toEndOf="@+id/rat_five"
        app:layout_constraintTop_toTopOf="@+id/rat_five" />

    <View
        android:id="@+id/view20"
        android:layout_width="0dp"
        android:layout_height="@dimen/_1sdp"
        android:layout_marginStart="@dimen/_17sdp"
        android:layout_marginTop="@dimen/_17sdp"
        android:layout_marginEnd="@dimen/_17sdp"
        android:background="@color/light_grey"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rat_five" />

    <TextView
        android:id="@+id/price_range_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_17sdp"
        android:fontFamily="@font/montserrat_medium"
        android:text="Price Range"
        android:layout_marginTop="@dimen/_17sdp"
        android:textColor="@color/wood_black"
        android:textSize="@dimen/_14ssp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view20" />

    <TextView
        android:id="@+id/price_range__hinttv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_17sdp"
        android:fontFamily="@font/montserrat_medium"
        android:text="@string/_500_1000"
        android:textColor="@color/grey"
        android:textSize="@dimen/_12ssp"
        app:layout_constraintEnd_toEndOf="@+id/view20"
        app:layout_constraintTop_toBottomOf="@+id/view20" />

    <com.google.android.material.slider.RangeSlider
        android:id="@+id/rangeSlider"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:valueFrom="0"
        android:valueTo="5000"
        app:layout_constraintEnd_toEndOf="@+id/view20"
        app:layout_constraintStart_toStartOf="@+id/price_range_tv"
        app:layout_constraintTop_toBottomOf="@+id/price_range_tv"
        app:values="@array/rangeSlider" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/montserrat_medium"
        android:text="0"
        android:textColor="@color/grey"
        android:textSize="@dimen/_12ssp"
        app:layout_constraintStart_toStartOf="@+id/rangeSlider"
        app:layout_constraintTop_toBottomOf="@+id/rangeSlider" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/montserrat_medium"
        android:text="5000$"
        android:textColor="@color/grey"
        android:textSize="@dimen/_12ssp"
        app:layout_constraintEnd_toEndOf="@+id/rangeSlider"
        app:layout_constraintTop_toBottomOf="@+id/rangeSlider" />

