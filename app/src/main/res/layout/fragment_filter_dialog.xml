        android:textColor="@color/wood_black"
        android:layout_marginStart="@dimen/_4sdp"
        android:textSize="@dimen/_12ssp"
        app:layout_constraintBottom_toBottomOf="@+id/rat_one"
        app:layout_constraintStart_toEndOf="@+id/rat_one"
        app:layout_constraintTop_toTopOf="@+id/rat_one" />

    <CheckBox
        android:id="@+id/rat_two"
        android:layout_width="@dimen/_20sdp"
        android:layout_height="@dimen/_20sdp"
        android:layout_marginTop="@dimen/_17sdp"
        android:layout_marginStart="@dimen/_7sdp"
        app:layout_constraintStart_toEndOf="@+id/rat_one_tv"
        app:layout_constraintTop_toBottomOf="@+id/rating_tv" />

    <TextView
        android:id="@+id/rat_two_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/montserrat_medium"
        android:text="4 Star"
        android:textColor="@color/wood_black"
        android:layout_marginStart="@dimen/_4sdp"
        android:textSize="@dimen/_12ssp"
        app:layout_constraintBottom_toBottomOf="@+id/rat_two"
        app:layout_constraintStart_toEndOf="@+id/rat_two"
